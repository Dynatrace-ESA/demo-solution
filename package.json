{
  "name": "@dt-esa/testing",
  "version": "1.0.0",
  "private": true,
  "esa-solution-server": {
    "title": "@dt-esa testing",
    "path": "/dtesa",
    "mode": "proxy",
    "image": "/doc/assets/dss.png",
    "description": "",
    "protocol": "https",
    "port": 6810
  },
  "scripts": {
    "prestart": "tsc",
    "start": "node index.js",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "dev:inspect": "nodemon --ext \"ts,json\" --ignore \"src /**/*.spec.ts\" --exec \"node --inspect=0.0.0.0:9229 --require ts-node/register ./src/index.ts\""
  },
  "dependencies": {
    "@dt-esa/authorizer": "^1.6.1",
    "@dt-esa/dynatrace-api-client": "^1.213.4",
    "@dt-esa/platform-constants": "^1.1.0",
